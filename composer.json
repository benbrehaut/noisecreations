{
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "luwes/craft3-inline": "v1.0.0",
    "wbrowar/adminbar": "3.1.6",
    "craftcms/contact-form": "2.2.2",
    "enovatedesign/craft-style-inliner": "1.1.1.1",
    "craftcms/contact-form-honeypot": "1.0.2",
    "craftcms/redactor": "2.1.6",
    "matt-west/craft-recaptcha": "v1.1.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
